ENV=dev
LOG_LEVEL=INFO

# Oanda
OANDA_ENV=practice
OANDA_API_KEY=your-oanda-api-key-here
OANDA_ACCOUNT_ID=your-oanda-account-id-here


# Engine
LOOP_INTERVAL_SECONDS=60
MAX_PAIRS=6

# Risk
RISK_PER_TRADE=0.01
MAX_DRAWDOWN_PER_DAY=0.03
MAX_OPEN_TRADES=5

# Defaults
DEFAULT_SL_PIPS=15
DEFAULT_TP_PIPS=30

# Liquidity engine (1 = execute trades on OANDA practice/live, 0 = signals only)
LIQUIDITY_TRADING_ENABLED=1
LIQUIDITY_PAPER_BALANCE=10000
LIQUIDITY_RISK_PCT=0.5
LIQUIDITY_MAX_UNITS_FX=2000
LIQUIDITY_MAX_UNITS_XAU=20
